# This for loop gives the date as key1 and dit[key1] is the {'Morning':'','Midday':'','Evening':''} 
for key1 in dit:
  nw={}
  # Here we have use the try and except statement, if it encounters error, then it will terminate and not store any news for the given day.
  try:
    for key2 in dit[key1]:
    
      id=dit[key1][key2]
      data=api.get_transcript(id)
  
      # print(data)
      para=''
      for elm in data:
        para=para+elm['text']+' '
  
      # para
      print(para)
      nw[key2]=para
    
  except:
    print(f"\nTranscripts are disabled for {key1}. Start now from next date.")
    break
   
  with open('para.csv','a+') as obj:
    for key2 in nw:
      obj.write(f"Euro News|{key1}|{key2}|Outside India|"+nw[key2]+"\n")

# Now we can directly import pandas and use it  
df=pd.read_csv('para.csv',sep='|',names=['Channel','Date','Day-time','Place','News'])
print(df)
